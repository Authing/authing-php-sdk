{
    "name": "my-authing-sdk/php",
    "description": "authing-php-sdk",
    "minimum-stability": "stable",
    "version": "5.0.0",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Han Fengkun",
            "email": "hanfengkun@authing.cn"
        }
    ],
    "autoload": {
        "psr-4": {
